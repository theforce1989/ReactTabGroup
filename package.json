    "babel-loader": "6.2.7",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "cpx": "^1.5.0",
    "css-loader": "0.25.0",
    "eslint": "3.9.1",
    "eslint-loader": "1.6.1",
    "eslint-plugin-lodash": "2.1.7",
